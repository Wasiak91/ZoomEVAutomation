<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="AllTests" verbose="1">
	<test name="redirectionsBenefitsToHomepage">
			<parameter name="browser"
			value="chrome" />
		<parameter name="startUrl"
			value="https://dev.zoom-ev.com/ev-benefits" />
						<parameter name="redirectButton" 
			value="//img[@alt='zoom logo colour long']" />
			<parameter name="targetUrl" 
			value="https://dev.zoom-ev.com/" />
		<parameter name="expectedPageName" value="Zoomev - Home" />
		<classes>
			<class name="com.zoomev.BenefitsRedirectionTests" />
		</classes>
	</test>

</suite>


