<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="SendingContactForm" verbose="1" parallel="tests" thread-count="2">
	<test name="redirectionsBenefitsToHomepage">
		<parameter name="browser" value="chrome" />
		<parameter name="contactPage"
			value="https://dev.zoom-ev.com/contact" />
		<parameter name="contactName"
			value="Automated Test" />
		<parameter name="telephone" value="123456789" />
		<parameter name="email" value="automatedest@test.com" />
		<parameter name="message" value="This message is generated by automated test" />
		<classes>
			<class name="com.zoomev.contactpage.SendingForm" />
		</classes>
	</test>
	
	</suite>
	