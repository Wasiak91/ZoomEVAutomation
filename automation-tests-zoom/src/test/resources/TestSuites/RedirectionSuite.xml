<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="AllRedirectionTests" verbose="1" parallel="tests" thread-count="5">
	<test name="redirectionsBenefitsToHomepage">
		<parameter name="browser" value="chrome" />
		<parameter name="startUrl"
			value="https://dev.zoom-ev.com/ev-benefits" />
		<parameter name="redirectButton"
			value="//img[@alt='zoom logo colour long']" />
		<parameter name="targetUrl" value="https://dev.zoom-ev.com/" />
		<parameter name="expectedPageName" value="ZoomEV - Home" />
		<classes>
			<class name="com.zoomev.redirectiontests.RedirectionTests" />
		</classes>
	</test>

